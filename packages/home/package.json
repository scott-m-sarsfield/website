{
  "name": "home",
  "version": "1.0.0",
  "private": true,
  "description": "home",
  "author": "Scott M Sarsfield",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) GIT_HASH=$(git rev-parse HEAD) gatsby develop",
    "start": "GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) GIT_HASH=$(git rev-parse HEAD) gatsby develop",
    "build": "GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) GIT_HASH=$(git rev-parse HEAD) gatsby build",
    "serve": "GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) GIT_HASH=$(git rev-parse HEAD) gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "build:static": "yarn run build && mkdir -p ../../dist && cp -R public/* ../../dist/",
    "format": "npx prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/material": "^5.15.7",
    "@scott-m-sarsfield/sherlock": "0.0.1",
    "@scott-m-sarsfield/three-fiber-test": "0.0.1",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-plugin-styled-components": "^2.1.3",
    "classnames": "^2.2.6",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gatsby": "^5.10.0",
    "gatsby-plugin-env-variables": "^2.3.0",
    "gatsby-plugin-image": "^3.10.0",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-mdx": "^5.10.0",
    "gatsby-plugin-sass": "^6.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-styled-components": "^6.10.0",
    "gatsby-source-filesystem": "^5.10.0",
    "gatsby-transformer-sharp": "^5.10.0",
    "lodash": "^4.17.20",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "styled-components": "^6.0.0-rc.3"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "prettier": "2.0.5",
    "typescript": "^5.0.4"
  }
}
